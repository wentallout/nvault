{
	"name": "nvault",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check .",
		"test": "npm run test:unit && npm run test:e2e",
		"test:unit": "vitest run",
		"test:unit:watch": "vitest",
		"test:e2e": "playwright test",
		"test:e2e:ui": "playwright test --ui",
		"test:coverage": "vitest run --coverage",
		"screenshots": "playwright test e2e/screenshot.test.ts --project=chromium-light chromium-dark",
		"screenshots:light": "playwright test e2e/screenshot.test.ts --project=chromium-light",
		"screenshots:dark": "playwright test e2e/screenshot.test.ts --project=chromium-dark"
	},
	"devDependencies": {
		"@fontsource/russo-one": "^5.2.5",
		"@iconify-json/iconoir": "^1.2.7",
		"@playwright/test": "^1.51.1",
		"@sveltejs/adapter-netlify": "^4.4.2",
		"@sveltejs/kit": "^2.20.2",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/svelte": "^5.2.7",
		"@unpic/svelte": "^1.0.0",
		"gsap": "^3.12.7",
		"jsdom": "^26.0.0",
		"postcss": "^8.5.3",
		"postcss-mixins": "^11.0.3",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.3.3",
		"svelte": "^5.25.3",
		"svelte-check": "^4.1.5",
		"sveltekit-view-transition": "^0.5.3",
		"typescript": "^5.8.2",
		"unplugin-icons": "^22.1.0",
		"vite": "^6.2.3",
		"vitest": "^3.0.9"
	},
	"dependencies": {
		"@fontsource-variable/sora": "^5.2.5"
	},
	"packageManager": "yarn@1.22.19+sha1.4ba7fc5c6e704fce2066ecbfb0b0d8976fe62447"
}
